<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" 
    android:padding="8dp"
    android:paddingBottom="12dp"
    xmlns:custom="http://schemas.android.com/apk/res/com.gidi.bio_console" >
    <RelativeLayout 
        android:id="@+id/change_artery_speed_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
         <TextView 
	        style="@style/style_title_txt"
	        android:paddingBottom="12dp"
	        android:id="@+id/artery_speed_title"
	        android:layout_alignParentLeft="true"
	        android:text="@string/left_kidney_artery_target_speed"
	        android:textColor="@color/white"/>
         <TextView 
            style="@style/style_pre_setting_title_txt"
            android:id="@+id/dialog_art_setting_speed_txt"
            android:layout_width="wrap_content"
		    android:layout_height="30dp"
            android:layout_toRightOf="@id/artery_speed_title"
            android:layout_marginLeft="16dp"
            android:text="@string/string_null"
            android:textColor="#FFD700"
            android:textSize="26sp"/>
         <RadioGroup 
		    android:id="@+id/dialog_art_speed_set_radiogroup"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_below="@id/artery_speed_title"
		    android:orientation="horizontal">
		    <RadioButton 
		        style="@style/preset_setting_temp_radiobutton"
		        android:id="@+id/dialog_art_set_speed_one_rb"
		        android:text="@string/number_0"/>
		    <RadioButton 
		        style="@style/preset_setting_temp_radiobutton"
		        android:id="@+id/dialog_art_set_speed_two_rb"
		        android:text="@string/number_600"
		        android:layout_marginLeft="24dp"/>
		   
		    <RadioButton 
		        style="@style/preset_setting_temp_radiobutton"
		        android:id="@+id/dialog_art_set_speed_three_rb"
		        android:text="@string/number_800"
		        android:layout_marginLeft="24dp"/>
		   <RadioButton 
		        style="@style/preset_setting_temp_radiobutton"
		        android:id="@+id/dialog_art_set_speed_four_rb"
		        android:text="@string/number_1200"
		        android:layout_marginLeft="24dp"/>
		  
		</RadioGroup>
         <com.gidi.bio_console.view.SystemKeyBoardEditText 
            style="@style/style_display_value_txt"
			android:id="@+id/target_artery_speed_edit"
			android:layout_width="162dp"
	        android:layout_height="52dp"
			android:layout_marginLeft="20dp"
			android:layout_below="@id/artery_speed_title"
			android:layout_toRightOf="@id/dialog_art_speed_set_radiogroup"
			android:background="@drawable/temp_choice_bg_selector"
			android:text="@string/string_null"
			android:textSize="20sp"
			android:textColor="@color/color_temp_choice_selector"
			android:textColorHint="@color/white"
			android:maxLength="4"
			android:inputType="number"
			custom:outSideCancel="true"
          	custom:keyDrawable="@drawable/btn_keyboard_key"
          	custom:xmlLayoutResId="@xml/keyboard_numbers" />
    </RelativeLayout>
    <RelativeLayout 
        android:id="@+id/change_vein_speed_layout"
        android:layout_width="match_parent"
        android:layout_height="260dp"

        android:layout_below="@id/change_artery_speed_layout">
        <TextView 
	        style="@style/style_title_txt"
	        android:id="@+id/vein_speed_title"
	        android:layout_alignParentLeft="true"
	        android:textColor="@color/white"
	        android:text="@string/right_kidney_artery_target_speed"        
	        android:paddingTop="12dp"
	    	android:paddingBottom="12dp"/>
        <TextView 
            style="@style/style_pre_setting_title_txt"
            android:id="@+id/dialog_vein_setting_speed_txt"
            android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:paddingTop="12dp"
	    	android:paddingBottom="12dp"
            android:layout_toRightOf="@id/vein_speed_title"
            android:layout_marginLeft="16dp"
            android:text="@string/string_null"
            android:textColor="#FFD700"
            android:textSize="26sp"/>
        <RadioGroup 
		    android:id="@+id/dialog_vein_speed_set_radiogroup"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_below="@id/vein_speed_title"
		    android:orientation="horizontal">
		    <RadioButton 
		        style="@style/preset_setting_temp_radiobutton"
		        android:id="@+id/dialog_vein_set_speed_one_rb"
		        android:text="@string/number_0"/>
		    <RadioButton 
		        style="@style/preset_setting_temp_radiobutton"
		        android:id="@+id/dialog_vein_set_speed_two_rb"
		        android:text="@string/number_600"
		        android:layout_marginLeft="24dp"/>
		   
		    <RadioButton 
		        style="@style/preset_setting_temp_radiobutton"
		        android:id="@+id/dialog_vein_set_speed_three_rb"
		        android:text="@string/number_800"
		        android:layout_marginLeft="24dp"/>
		   <RadioButton 
		        style="@style/preset_setting_temp_radiobutton"
		        android:id="@+id/dialog_vein_set_speed_four_rb"
		        android:text="@string/number_1200"
		        android:layout_marginLeft="24dp"/>
		  
		</RadioGroup>
		<com.gidi.bio_console.view.SystemKeyBoardEditText
            style="@style/style_display_value_txt"
			android:id="@+id/target_vein_speed_edit"
			android:layout_width="162dp"
	        android:layout_height="52dp"
			android:layout_below="@id/vein_speed_title"
			android:layout_toRightOf="@id/dialog_vein_speed_set_radiogroup"
			android:background="@drawable/temp_choice_bg_selector"
			android:layout_marginLeft="20dp"
			android:maxLength="4"
			android:text="@string/string_null"
			android:textSize="20sp"
			android:textColor="@color/color_temp_choice_selector"
			android:textColorHint="@color/white"
			android:inputType="number"
			custom:outSideCancel="true"
          	custom:keyDrawable="@drawable/btn_keyboard_key"
          	custom:xmlLayoutResId="@xml/keyboard_numbers"/>
    </RelativeLayout>

</RelativeLayout>

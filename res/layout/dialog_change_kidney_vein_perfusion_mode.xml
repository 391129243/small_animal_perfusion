<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="12dp" >
	<!-- 门静脉灌注模式 -->
   <RadioGroup 
        android:id="@+id/dialog_vein_mode_rg"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <RadioButton 
            style="@style/style_setting_mode_rb"
            android:id="@+id/dialog_vein_const_pre_rb"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:text="@string/preset_vein_const_pressure"
            android:paddingRight="24dp"/>
		<RadioButton 
            style="@style/style_setting_mode_rb"
            android:id="@+id/dialog_vein_pulse_pressure_rb"
            android:layout_weight="1"
            android:paddingLeft="24dp"
            android:text="@string/preset_artery_pulse_pressure"/>	
        <RadioButton 
            style="@style/style_setting_mode_rb"
            android:id="@+id/dialog_vein_const_flow_rb"
            android:layout_weight="1"
            android:paddingLeft="24dp"
            android:text="@string/preset_vein_const_flow"/>

    </RadioGroup>

	<RelativeLayout
	    android:id="@+id/dlg_setting_vein_layout"
	    android:layout_below="@id/dialog_vein_mode_rg"
	    android:layout_width="950dp"
	    android:layout_height="wrap_content"
	    android:paddingBottom="150dp"
	    android:layout_marginTop="18dp">
		<TextView 
            style="@style/style_pre_setting_title_txt"
            android:id="@+id/dlg_setting_vein_title"
            android:text="@string/preset_pattern_right_target_artery_pressure"/>
		<TextView 
            style="@style/style_setting_dlg_text"
            android:id="@+id/dlg_setting_vein_target_txt"
            android:layout_toRightOf="@id/dlg_setting_vein_title"
            android:layout_marginLeft="16dp"
            android:text="@string/string_null"/>	
        <!-- 右肾动脉显示最大最小值 -->
        <TextView 
            style="@style/style_setting_dlg_text"
            android:id="@+id/dlg_setting_vein_max_tv"
            android:layout_toRightOf="@id/dlg_setting_vein_title"
            android:layout_marginLeft="16dp"
            android:visibility="gone"
	        android:text="@string/string_null"/>
       <TextView 
            style="@style/style_setting_dlg_text"
            android:id="@+id/dlg_setting_vein_divider_tv"
            android:layout_toRightOf="@id/dlg_setting_vein_max_tv"
            android:layout_marginLeft="16dp"
            android:visibility="gone"
	        android:text="/"/>        
        <TextView 
            style="@style/style_setting_dlg_text"
            android:id="@+id/dlg_setting_vein_min_tv"
            android:layout_toRightOf="@id/dlg_setting_vein_divider_tv"
            android:layout_marginLeft="16dp"
            android:visibility="gone"
	        android:text="@string/string_null"/>     
        <!-- 右肾动脉恒压 -->    
        <LinearLayout 
            android:id="@+id/dlg_setting_vein_pressure_layout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:visibility="gone"
            android:layout_below="@id/dlg_setting_vein_title"
	        android:layout_marginTop="18dp"
            android:orientation="horizontal">
            <RadioGroup 
			    android:id="@+id/dlg_set_vein_pre_radiogroup"
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:orientation="horizontal">
				<RadioButton 
			        style="@style/preset_setting_temp_radiobutton"
			        android:id="@+id/dlg_set_vein_pre_one_rb"
			        android:text="@string/number_20"/>

			    <RadioButton
			        android:id="@+id/dlg_set_vein_pre_two_rb"
			        style="@style/preset_setting_temp_radiobutton"
			        android:layout_marginLeft="22dp"
			        android:text="@string/number_40" />
			   
			    <RadioButton 
			        style="@style/preset_setting_temp_radiobutton"
			        android:id="@+id/dlg_set_vein_pre_three_rb"
			        android:text="@string/number_60"
			        android:layout_marginLeft="22dp"/>
			   <RadioButton 
			        style="@style/preset_setting_temp_radiobutton"
			        android:id="@+id/dlg_set_vein_pre_four_rb"
			        android:text="@string/number_80"
			        android:layout_marginLeft="22dp"/>	
		   </RadioGroup>
		   <com.gidi.bio_console.view.SystemKeyBoardEditText 
	            android:id="@+id/dlg_setting_vein_target_pressure_et"
	            android:layout_width="106dp"
	            android:layout_height="52dp"
				android:layout_marginLeft="22dp"
	            android:gravity="center"
	            android:textColor="@color/color_temp_choice_selector"
	            android:textColorHint="@color/white"
	            android:textSize="18sp"
	            android:inputType="numberDecimal"
	            android:maxLength="4"
	            android:hint="@string/custom_pressure"
	            android:background="@drawable/temp_choice_bg_selector"
	            custom:outSideCancel="true"
           		custom:keyDrawable="@drawable/btn_keyboard_key"
           		custom:xmlLayoutResId="@xml/keyboard_numbers"/> 
        </LinearLayout>
		<!-- 右肾动脉恒流 -->  
        <LinearLayout 
                 android:id="@+id/dlg_setting_vein_flow_layout"
                 android:layout_width="fill_parent"
                 android:layout_height="wrap_content"
                 android:layout_below="@id/dlg_setting_vein_title"
                 android:orientation="horizontal"
                 android:layout_marginTop="18dp"
                 android:visibility="gone">
              <RadioGroup 
			    	android:id="@+id/dlg_set_vein_flow_radiogroup"
			    	android:layout_width="wrap_content"
			    	android:layout_height="wrap_content"
			    	android:orientation="horizontal">
					<RadioButton 
			        	style="@style/preset_setting_temp_radiobutton"
			        	android:id="@+id/dlg_set_vein_flow_one_rb"
			        	android:text="@string/number_200"/>

			    	<RadioButton
			        	android:id="@+id/dlg_set_vein_flow_two_rb"
			        	style="@style/preset_setting_temp_radiobutton"
			        	android:layout_marginLeft="22dp"
			        	android:text="@string/number_400" />
			   
			    	<RadioButton 
			       	 	style="@style/preset_setting_temp_radiobutton"
			        	android:id="@+id/dlg_set_vein_flow_three_rb"
			        	android:text="@string/number_600"
			        	android:layout_marginLeft="22dp"/>
			   		<RadioButton 
			        	style="@style/preset_setting_temp_radiobutton"
			        	android:id="@+id/dlg_set_vein_flow_third_rb"
			        	android:text="@string/number_800"
			        	android:layout_marginLeft="22dp"/>	
		   </RadioGroup>
		   <com.gidi.bio_console.view.SystemKeyBoardEditText
            	android:id="@+id/dlg_setting_vein_target_flow_et"
           	 	android:layout_width="106dp"
            	android:layout_height="52dp"		
            	android:layout_marginLeft="22dp"
            	android:gravity="center"
            	android:textColor="@color/color_temp_choice_selector"
            	android:textColorHint="@color/white"
            	android:textSize="18sp"
            	android:inputType="number"           
            	android:maxLength="5"
            	android:hint="@string/custom_flow"
            	android:background="@drawable/temp_choice_bg_selector"
            	custom:outSideCancel="true"
          		custom:keyDrawable="@drawable/btn_keyboard_key"
          		custom:xmlLayoutResId="@xml/keyboard_numbers"/>            			
             </LinearLayout>
             <!-- 右肾动脉搏动 -->
      		<RelativeLayout 
			    android:id="@+id/dlg_setting_vein_pulse_pressure_layout"
			    android:layout_width="fill_parent"
	        	android:layout_height="wrap_content"
	        	
	        	android:layout_below="@id/dlg_setting_vein_title">
			    <TextView 
		            style="@style/style_pre_setting_title_txt"
		            android:id="@+id/setting_vein_max_pressure_title"
		            android:text="@string/max"/>
				<RadioGroup 
				    android:id="@+id/dlg_set_vein_pulse_max_pre_radiogroup"
				    android:layout_width="wrap_content"
				    android:layout_height="wrap_content"
				    android:layout_below="@id/setting_vein_max_pressure_title"
				    android:layout_marginTop="12dp"
				    android:orientation="horizontal">
					<RadioButton 
				        style="@style/preset_setting_temp_radiobutton"
				        android:id="@+id/dlg_set_vein_pulse_max_one_rb"
				        android:text="@string/number_40"/>
	
				    <RadioButton
				        android:id="@+id/dlg_set_vein_pulse_max_two_rb"
				        style="@style/preset_setting_temp_radiobutton"
				        android:layout_marginLeft="22dp"
				        android:text="@string/number_60" />
				   
				    <RadioButton 
				        style="@style/preset_setting_temp_radiobutton"
				        android:id="@+id/dlg_set_vein_pulse_max_three_rb"
				        android:text="@string/number_80"
				        android:layout_marginLeft="22dp"/>
				   <RadioButton 
				        style="@style/preset_setting_temp_radiobutton"
				        android:id="@+id/dlg_set_vein_pulse_max_four_rb"
				        android:text="@string/number_100"
				        android:layout_marginLeft="22dp"/>	
			   </RadioGroup>
		       <com.gidi.bio_console.view.SystemKeyBoardEditText 
		            android:id="@+id/dlg_setting_vein_max_tarpressure_et"
		            android:layout_width="106dp"
		            android:layout_height="52dp"
		            android:paddingLeft="2dp"
	            	android:paddingRight="2dp"
	            	android:layout_marginLeft="22dp"
	            	android:layout_marginTop="12dp"
		            android:layout_below="@id/setting_vein_max_pressure_title"
		            android:layout_toRightOf="@id/dlg_set_vein_pulse_max_pre_radiogroup"
		            android:gravity="center"
		            android:textColor="@color/color_temp_choice_selector"
		            android:textSize="18sp"
		            android:inputType="number"
		            android:maxLength="3"
		            android:hint="@string/custom_pressure"
		            android:textColorHint="@color/white"	            
		            android:background="@drawable/temp_choice_bg_selector"
		            custom:outSideCancel="true"
	           		custom:keyDrawable="@drawable/btn_keyboard_key"
	           		custom:xmlLayoutResId="@xml/keyboard_numbers"/> 
	           	<TextView 
		            style="@style/style_pre_setting_title_txt"
	            	android:id="@+id/setting_vein_min_pressure_title" 
		            android:layout_below="@id/dlg_set_vein_pulse_max_pre_radiogroup" 
					android:layout_marginTop="12dp"
		            android:text="@string/min"/>
				<RadioGroup 
				    android:id="@+id/dlg_set_vein_pulse_min_pre_radiogroup"
				    android:layout_width="wrap_content"
				    android:layout_height="wrap_content"
				    android:layout_below="@id/setting_vein_min_pressure_title"
				    android:layout_marginTop="12dp"
				    android:orientation="horizontal">
					<RadioButton 
				        style="@style/preset_setting_temp_radiobutton"
				        android:id="@+id/dlg_set_vein_pulse_min_one_rb"
				        android:text="@string/number_20"/>
	
				    <RadioButton
				        android:id="@+id/dlg_set_vein_pulse_min_two_rb"
				        style="@style/preset_setting_temp_radiobutton"
				        android:layout_marginLeft="22dp"
				        android:text="@string/number_40" />
				   
				    <RadioButton 
				        style="@style/preset_setting_temp_radiobutton"
				        android:id="@+id/dlg_set_vein_pulse_min_three_rb"
				        android:text="@string/number_60"
				        android:layout_marginLeft="22dp"/>
				   <RadioButton 
				        style="@style/preset_setting_temp_radiobutton"
				        android:id="@+id/dlg_set_vein_pulse_min_four_rb"
				        android:text="@string/number_80"
				        android:layout_marginLeft="22dp"/>	
			   </RadioGroup>	
		       <com.gidi.bio_console.view.SystemKeyBoardEditText 
		            android:id="@+id/dlg_setting_vein_min_tarpressure_et"
		            android:layout_width="106dp"
		            android:layout_height="52dp"
		            android:layout_below="@id/setting_vein_min_pressure_title"
		            android:layout_toRightOf="@id/dlg_set_vein_pulse_min_pre_radiogroup"
		            android:layout_marginLeft="22dp"
		            android:layout_marginTop="12dp"
		            android:paddingLeft="2dp"
		            android:paddingRight="2dp"
		            android:gravity="center"
		            android:textColor="@color/color_temp_choice_selector"
		            android:textColorHint="@color/white"
		            android:textSize="18sp"
		            android:inputType="number"
		            android:maxLength="3"
		            android:hint="@string/custom_pressure"
		            android:background="@drawable/temp_choice_bg_selector"
		            custom:outSideCancel="true"
	           		custom:keyDrawable="@drawable/btn_keyboard_key"
	           		custom:xmlLayoutResId="@xml/keyboard_numbers"/>   				              	
				</RelativeLayout>
	</RelativeLayout>

</RelativeLayout>

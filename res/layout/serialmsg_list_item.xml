<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" 
    android:gravity="center_vertical"
    android:background="@color/color_serial_msg_odd_bg">
    <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/seial_msg_time"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:singleLine="false"
        android:layout_weight="2"
        android:text="@string/msg_time"/>  

    <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_meanpre_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
   <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_syst_pre_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
   <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_dias_pre_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
   <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_prefreq_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"/>
   <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_artflow_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
   <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_artspeed_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
   <!-- 肝动脉阻力指数 -->
   <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_artresistindex_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1.5"/>
   <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_seial_artery_ehbf_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
   <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_vein_meanpre_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
   <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_veinflow_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
  <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_veinspeed_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
   <!-- 门静脉阻力指数 -->
   <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_veinresistindex_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1.5"/>
   	<TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_seial_vein_ehbf_content_txt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
   <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/artery_temp_text"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"/>

</LinearLayout>

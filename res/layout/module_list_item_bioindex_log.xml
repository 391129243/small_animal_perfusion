<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:paddingTop="8dp"
    android:paddingBottom="8dp"
    android:layout_height="76dp"
    android:gravity="center_vertical"
    android:orientation="horizontal"
    android:background="@drawable/bioindex_log_item_even_bg_selector">
     <!-- 参数详情 -->
	<TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_bioindex_log_index"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"/>  
    <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_bioindex_log_date"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
    <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_bioindex_patient_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
    <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_bioindex_operator_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
    <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_bioindex_specimen_id"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>
    <TextView 
        style="@style/serialmsg_list_item_style"
        android:id="@+id/item_bioindex_specimen_source"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"/>

</LinearLayout>
